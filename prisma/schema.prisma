datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model weatherData {
  id         Int      @id @default(autoincrement())
  location   String
  date       String
  humidity   Float
  dewpoint_c Float
  gust_kph   Float
  uv         Float
  sunrise    String
  sunset     String
  temp_c     Float
}
